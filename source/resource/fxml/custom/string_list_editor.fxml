<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.Double?>
<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.collections.FXCollections?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<fx:root type="VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <stylesheets>
    <URL value="@../../css/main/modification.css"/>
  </stylesheets>
  <spacing><Double rpx="5"/></spacing>
  <ListView fx:id="$stringView" orientation="horizontal">
    <prefHeight><Double rpx="31"/></prefHeight>
    <minHeight><Double rpx="31"/></minHeight>
  </ListView>
  <HBox>
    <spacing><Double rpx="5"/></spacing>
    <TextField fx:id="$additionControl" HBox.hgrow="always"/>
    <Button text="追加" onAction="#add">
      <prefWidth><Double rpx="70"/></prefWidth>
      <minWidth><Double rpx="70"/></minWidth>
    </Button>
    <Button text="分割追加" onAction="#addSeparate">
      <prefWidth><Double rpx="70"/></prefWidth>
      <minWidth><Double rpx="70"/></minWidth>
    </Button>
  </HBox>
</fx:root>